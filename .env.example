# Configuração do Servidor
PORT=3011
NODE_ENV=development
PORT_FRONTEND=3010
NODE_ENV=development

# Provedor de IA Ativo
PROVIDER=openrouter

# OpenAI
OPENAI_API_KEY=sk-proj...
OPENAI_MODEL=gpt-4
OPENAI_EMBEDDING_MODEL=text-embedding-3-large

# Anthropic
ANTHROPIC_API_KEY=sk-ant-...
ANTHROPIC_MODEL=claude-3-opus-20240229
ANTHROPIC_EMBEDDING_MODEL=openai  # Nome do provedor para delegar embeddings

# Ollama
OLLAMA_URL=http://localhost:11434
OLLAMA_MODEL=granite3.3:8b
OLLAMA_EMBEDDING_MODEL=granite-embedding:278m

# OpenRouter
OPENROUTER_API_KEY=sk-or-...
OPENROUTER_BASE_URL=https://openrouter.ai/api/v1
OPENROUTER_MODEL=mistralai/mistral-7b-instruct
OPENROUTER_EMBEDDING_MODEL=text-embedding-3-small

# Perplexity
PERPLEXITY_API_KEY=pplx-...
PERPLEXITY_BASE_URL=https://api.perplexity.ai
PERPLEXITY_MODEL=pplx-70b-online
PERPLEXITY_EMBEDDING_MODEL=ollama  # Nome do provedor para delegar embeddings

# OpenWebUI
OPENWEBUI_API_KEY=sk-...
OPENWEBUI_BASE_URL=http://localhost:8080/api/v1
OPENWEBUI_MODEL=gpt-4
OPENWEBUI_EMBEDDING_MODEL=granite-embedding:278m

# GenerAtiva
GENERATIVA_API_KEY=sk-...
GENERATIVA_BASE_URL=https://generativa.rapport.tec.br/v1
GENERATIVA_MODEL=gpt-4
GENERATIVA_EMBEDDING_MODEL=granite-embedding:278m
# Caminhos de Configuração (opcional - sobrescreve padrão /etc/rapport/genai-eng-prompt/)
# CONFIG_PATH=/caminho/para/config.json
# ADS_CONFIG_PATH=/caminho/para/ads.conf
